<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lautung.rxjavademo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.lautung.rxjavademo.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.lautung.rxjavademo.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.lautung.rxjavademo.ui.retrofit.RetrofitTestActivity"/>
        <activity android:name="com.lautung.rxjavademo.ui.retrofit.RetrofitLoginActivity"/>

        <activity android:name="com.lautung.rxjavademo.ui.rxbinding.ButtonClicksActivity"/>
        <activity android:name="com.lautung.rxjavademo.ui.rxbinding.LoginActivity"/>
        <activity android:name="com.lautung.rxjavademo.ui.rxbinding.RegisterActivity"/>
        <activity android:name="com.lautung.rxjavademo.ui.rxbinding.FilterActivity"/>


        <activity android:name="com.lautung.rxjavademo.ui.op.OpActivity"/>
    </application>

</manifest>